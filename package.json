{
  "name": "js-word-counter",
  "displayName": "JS Word Counter",
  "description": "Compute the word count and density of your JS code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:js-word-counter.computeIdentifierDensity"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "js-word-counter.computeIdentifierDensity",
        "title": "Compute JS Identifier Density"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.61.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.17.27",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "@vscode/test-electron": "^1.6.2"
  },
  "dependencies": {
    "@babel/parser": "^7.16.0",
    "@babel/traverse": "^7.16.0"
  }
}
